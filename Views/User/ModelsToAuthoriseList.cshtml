
@Html.Partial("Header")
@{
    Style.Require("ExpeditGeology");

    Layout.Title = T("Models Awaiting Authorisation").ToString();
}
@if (AuthorizedFor(NKD.Permissions.ManageProjects)) {
    <h3>@Layout.Title</h3>
    Html.RenderPartial("ModelsToAuthoriseListPartial");
}
else {
    
    Html.RenderAction("UnauthorizedRedirect");
}